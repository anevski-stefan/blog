{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "next.config.{js,mjs,ts}",
    "next-sitemap.config.mjs",
    "app/**/*.{js,jsx,ts,tsx}",
    "middleware.{js,ts}",
    "instrumentation.{js,ts}"
  ],
  "project": [
    "src/**/*.{js,jsx,ts,tsx}",
    "app/**/*.{js,jsx,ts,tsx}",
    "!src/generated/**"
  ],
  "ignoreDependencies": [
    "@tailwindcss/typography",
    "eslint-config-next",
    "sharp",
    "tw-animate-css"
  ],
  "ignoreExportsUsedInFile": true,
  "ignore": [".next/**", "node_modules/**"]
}
